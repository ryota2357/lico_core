{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "groupName": "patch dependencies",
      "matchDepTypes": ["dependencies", "dev-dependencies", "workspace.dependencies"],
      "matchUpdateTypes": ["patch"]
    }
  ]
}
